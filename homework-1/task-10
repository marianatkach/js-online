// task-10
function buble()
{
    var arr= new Array(1,2,3,4,5);
    var i=0;
     while(i<arr.length)
    {
        if((arr[i])<(arr[i+1]))
        {
            var tmp=arr[i];
            arr[i]=arr[i+1];
            arr[i+1]=tmp;
            i=(i>=1)?i-1:i+1;
        }else
        {
            i++;
        }
    }
    alert(arr.toString());
}
buble();
